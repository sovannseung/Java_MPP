
public class Paycheck {

		private double grossPay;
		private double fica;
		private double state;
		private double local;
		private double medicare;
		private double socialSecurity;
		private String payPeriod;
		
		public Paycheck(double grossPay, double fica, double state, double local, double medicare, double socialSecurity, String payPeriod) {
			this.grossPay = grossPay;
			this.fica = fica;
			this.state = state;
			this.local = local;
			this.medicare = medicare;
			this.socialSecurity = socialSecurity;
			this.payPeriod = payPeriod;
		}

		public double getGrossPay() {
			return grossPay;
		}

		public double getFica() {
			return fica;
		}

		public double getState() {
			return state;
		}

		public double getLocal() {
			return local;
		}

		public double getMedicare() {
			return medicare;
		}

		public double getSocialSecurity() {
			return socialSecurity;
		}

		public String getPayPeriod() {
			return payPeriod;
		}
		
		public void print() {
			System.out.println("Paycheck [grossPay=" + grossPay + ", fica=" + fica + ", state=" + state + ", local=" + local
					+ ", medicare=" + medicare + ", socialSecurity=" + socialSecurity + ", payPeriod=" + payPeriod
					+ ", getNetPay()=" + getNetPay() + "]");
		}
		
		public double getNetPay() {
			return grossPay - (fica * 0.23) - (state * 0.05) - (local * 0.01) - (medicare * 0.03) - (socialSecurity * 0.075); 
		}
		
}
